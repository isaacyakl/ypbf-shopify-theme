}

/* Tables */

table {
  background: transparent;
  color: $body-color;
  border: 1px solid $border-color;
  border-top: none;
  border-radius: 5px;
  border-collapse: separate;
  font-size: 14px;
  line-height: 22px;

  &.clean {
    border: none;
    border-radius: 0;
  }

  .column-title {
    display: none;
  }
}

td, th {
  border-top: 1px solid $border-color;

  .clean & {
    border: none;
  }
}

thead {

  th:first-child {
    border-radius: 5px 0 0 0;
  }

  th:last-child {
    border-radius: 0 5px 0 0;
  }
}

tbody tr:first-child {

  td:first-child,
  th:first-child {
    border-radius: 5px 0 0 0;
  }

  td:last-child,
  th:last-child {
    border-radius: 0 5px 0 0;
  }

  thead + & {
    td:first-child,
    th:first-child,
    td:last-child,
    th:last-child {
      border-radius: 0;
    }
  }
}

tbody tr:last-child {

  td:first-child,
  th:first-child {
    border-radius: 0 0 0 5px;
  }

  td:last-child,
  th:last-child {
    border-radius: 0 0 5px 0;
  }
}

th {
  font-weight: normal;
  padding: 15px;
  color: $heading-color;

  @include compliment($body-background-color);
}

td {
  border-left: 1px solid $border-color;
  padding: 30px;

  &:first-child {
    border-left: none;
  }
}

table.mobile-layout {
  border-bottom: none;

  .column-title {
    font-weight: 700;
    display: inline-block;
  }

  thead {
    display: none;
  }

  tr td {
    width: 100%;
    display: block;
    text-align: left;
    border-top: 1px solid $border-color !important;
    border-left: none;

    /* < IE 10 fix */
    float:left;
    clear:left;
  }

  td:last-child {
    border-bottom: none;
  }

  tr:last-child td:last-child {
    border-bottom: 1px solid $border-color !important;
  }
}

