/* Forms & buttons */

form {

}

.input-wrapper {
  margin: 25px 0;

  label {
    display: block;
    margin-bottom: 10px;
  }
}

.input-group-wrapper {
  margin: 25px 0;

  span {
    display: block;
  }

  label {
    display: block;
    font-size: 14px;
    margin: 11px 0;
  }

  input {
    margin-right: 5px;
  }
}

input, textarea {
  width: 100%;
  max-width: 670px;
  font-family: inherit;

  &[type="checkbox"],
  &[type="radio"] {
    width: auto;
  }

  &:focus {
    outline: none;
  }
}

input[type="text"],
input[type="email"],
input[type="password"],
input[type="search"],
input[type="telephone"],
input[type="tel"],
input[type="number"],
textarea {
  -webkit-appearance: none;
  border: 1px solid $border-color;
  border-radius: 5px;
  background: $body-background-color;
  color: $body-color;
  display: block;
  padding: 14px 15px 9px;
  font-size: 14px;

  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;

  @include placeholder {
    color: ligthen($body-color,30%);
  }

  &.error,
  .error & {
    color: $error-color;
    border: 1px solid $error-color;

    @include placeholder {
      color: lighten($error-color,30%);
    }
  }

  @include breakpoint(s) {
    font-size: 16px; // removes zoom on focus in iOS
  }
}

.error-message,
.errors {
  color: $error-color;
}

input[type="password"] {
  @include lt-ie9 {
    font-family: Arial, sans-serif;
  }
}

textarea {

}

select {
  max-width: 100%;

  @include breakpoint(s) {
    font-size: 16px; // removes zoom on focus in iOS
  }
}

.select-wrapper {
  cursor: pointer;
  border: 1px solid $border-color;
  position: relative;
  font-size: 14px;
  border-radius: 5px;
  padding: 14px 15px 9px;

  label + & {
    margin-top: 10px;
  }
  
  .selected-text:after { /* Added down arrow by Yak */
    content: '\E006';
    margin-left: 10px;
    @include icon-font();
  }

  @include compliment($body-background-color);

  &:after {
    content: '\E007';
    position: absolute;
    right: 15px;
    top: 50%;
    margin-top: -7px;
    @include icon-font();
  }

  select {
    @extend %cover;
    color: black;
    opacity: 0;
    filter: alpha(opacity=0); /* IE8 */
    cursor: pointer;
    z-index: 2;
    width: 100%; /* needed for Firefox */
    height: 100%; /* needed for IE */

    -webkit-appearance: none;
  }

}

/* iframes and embeds */

iframe {
  border: none;
}

/* RTE areas */

.rte {

  @extend .clearfix;

  > *:first-child {
    margin-top: 0;
  }

  > *:last-child {
    margin-bottom: 0;
  }

  ul {
    list-style: disc;
  }

  ul, ol, dl {

  }

  li {
    margin: 11px 0;
  }

  blockquote {

  }

}

* {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}

body {
  background-image: $body-background-image;
  background-color: $body-background-color;
  background-repeat: repeat;
  color: $body-color;

  @include retina {
    background-image: $body-background-image-retina;
    background-size: $body-background-image-size;
  }

  &.modal-overflow {
    overflow: hidden;
    position: relative;
  }
}

.main-content-wrapper {
  position: relative;
  /*padding: 0 30px;*/
  padding-top: 20px;
  padding-bottom: 100px;
  padding-left: 30px;
  padding-right: 30px;
  min-width: $min-width;
  max-width: $max-width + 60; // +60 for padding
  margin: 0 auto;
 

  @include lt-ie9 {
    max-width: $max-width;
  }

  @include breakpoint(m) {
    /*padding: 0 15px;*/
  }
}

.main-content {
  /*width: 65%;*/ /* Commented out by Yak */
  margin: 0 auto;

  @include breakpoint(l) {
    width: 80%;
  }

  @include breakpoint(m) {
    width: 100%;
  }

  .sidebar-enabled & {
    padding-bottom: 50px;
  }
}

.rte .tabs {
  display: block;
  width: 100%;
  height: 42px;
  padding: 0;
  margin: 30px 0 0 30px;
  white-space: nowrap;
  position: relative;
  z-index: 10;

  li {
    display: inline-block;
    height: 41px;
    font-size: 14px;
    line-height: 42px;
    padding: 0 16px;
    margin: 0;
    border: 1px solid transparent;
    border-bottom: none;
    cursor: pointer;
    color: $heading-color;

    &.active {
      border-color: $border-color;
      height: 42px;
      border-radius: 5px 5px 0 0;
      background: $body-background-color;
    }
  }
}

.rte .tabs-content {
  display: block;
  width: 100%;
  position: relative;
  top: -1px;
  z-index: 5;
  padding: 30px;
  margin: 0 0 30px;
  overflow: hidden;
  border: 1px solid $border-color;
  border-radius: 5px;

  > li {
    display: none;
    margin: 0;

    > *:first-child {
      margin-top: 0;
    }

    > *:last-child {
      margin-bottom: 0;
    }

    &.active { display: block; }
  }
}

.modal-wrapper {
  display: none;
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background: rgba($body-background-color, 0.7);
  opacity: 0;
  z-index: 9999;

  @include transition(opacity 0.3s $ease-out-quad);

  &.active,
  &.opening {
    display: block;
  }

  &.opening,
  &.open {
    opacity: 1;
  }

  .modal-overflow & {
    overflow-y: scroll;
    -webkit-overflow-scrolling: touch;
  }

  @include breakpoint(s) {
    padding-bottom: 30px;
  }
}

.modal-content {
  position: absolute;
  top: 55%;
  left: 50%;
  width: 60%;
  max-width: 640px;
  padding: 30px;
  background: $body-background-color;
  border: 1px solid $border-color;
  opacity: 0;

  @include breakpoint(l) {
    width: 70%;
  }

  @include breakpoint(m) {
    width: 80%;
  }

  @include breakpoint(s) {
    position: relative;
    margin: 0 0 30px !important;
    top: 15px !important;
    left: 15px !important;
    width: -webkit-calc(100% - 30px);
    width: calc(100% - 30px);
    padding: 70px 15px 15px;

    @include lt-ie9 {
      width: 100%;
    }
  }

  @include transition(opacity 0.3s $ease-out-quad, top 0.3s $ease-out-quad);

  .open & {
    top: 50%;
    opacity: 1;
  }

  .modal-overflow & {
    top: 30px!important;
    margin-bottom: 30px;
  }
}

.modal-close-secondary {
  @extend .meta;
  position: absolute;
  top: 30px;
  right: 30px;
  line-height: 1;
  cursor: pointer;
  color: $meta-color-secondary;

  &:hover {
    color: $meta-color;
  }

  &:after {
    content: '\E010';
    top: 3px;
    position: relative;
    margin-left: 5px;
    @include icon-font(25px, 0);
  }

  @include breakpoint(s) {
    right: 50%;
    margin-right: -35px;
  }
}

.modal-title {
  margin-top: 0;
}


