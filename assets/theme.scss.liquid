@include breakpoint(xl) {

  .navigation {
    text-align: center;
    /*margin-top: 25px;*/
    /*padding: 5px;*/
    padding-bottom: 10px; /* added by Yak */
    padding-top: 10px; /* added by Yak */
    margin-top: 15px; /* added by Yak */
    width: 100%;
    
    background: #8fca5b; /* Old browsers */
    background: -moz-linear-gradient(top,  #8fca5b 1%, #7cb84d 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(1%,#8fca5b), color-stop(100%,#7cb84d)); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top,  #8fca5b 1%,#7cb84d 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top,  #8fca5b 1%,#7cb84d 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(top,  #8fca5b 1%,#7cb84d 100%); /* IE10+ */
    background: linear-gradient(to bottom,  #8fca5b 1%,#7cb84d 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#8fca5b', endColorstr='#7cb84d',GradientType=0 ); /* IE6-9 */
    
    border-radius: 5px 5px 0px 0px; /* Added by Yak */

    ul {
      margin: 0;
      padding: 0;
    }

    li {
      position: relative;

      &:hover > a {
        color: $accent-color;
      }

      a:hover {
        color: $accent-color;
      }
    }

    a {
      display: block;
    }
  }

  .has-dropdown {
       
    &:hover {
      > a {
        position: relative;
      }

      > ul {        
        visibility:visible;
  		opacity:1;
        z-index: 5;
        width: 230px;
        overflow: visible;
        
        /* Add properties here and they
        will affect all tiers of menus */
        
        /*display: block;*/
        
        -webkit-box-shadow: 2px 3px 5px 0px rgba(0,0,0,0.2);
		-moz-box-shadow: 2px 3px 5px 0px rgba(0,0,0,0.2);
		box-shadow: 2px 3px 5px 0px rgba(0,0,0,0.2);
      }
      
      /* Menu appearing settings */
      > ul.navigation-first-tier,ul.navigation-second-tier {
        transition-delay: 0.2s; /*delay before dropdown menu becomes visible*/
        transition-duration: 0s; /*make menus animation open immediately*/
      }

      > ul.navigation-third-tier,ul.navigation-fourth-tier {
        transition-delay:0.3s;
        transition-duration: 0s;
      }
    }
  }

  /* Inside any dropdown */
  .regular-nav ul {
    /*display: none;*/
    
	/* You could put one line for menu disappearing settings here
    and it would apply to all tiers */
    transition: visibility 0s linear 0.7s,opacity 0.2s linear 0.5s,box-shadow 0.2s linear 0.5s,width 0s linear 0.7s;
    
    visibility:hidden;
  	opacity:0;
    z-index: 4;
    width: 0px;
    overflow: hidden;
    
    position: absolute;
    background: $body-background-color;
    border-bottom: 1px solid $border-color;
    width: 230px;
    
    text-align: left;
    /*font-size: 14px;*/
    font-size: 1em; /* added by Yak */

    a {
      /*padding: 10px 20px;*/
      padding: 1px 20px 1px 20px; /* Added by Yak */
      /*border: 1px solid $border-color;*/
      border: 1px solid #F2F2F2; /* Added by Yak */
      border-bottom: none;
      color: #000; /* added by Yak */
    }
    
    & .navigation-second-tier {
      transition: visibility 0s linear 0.7s,opacity 0.2s linear 0.5s,box-shadow 0.2s linear 0.5s,width 0s linear 0.7s;
    }
    
    & .navigation-third-tier {
      transition: visibility 0s linear 0.7s,opacity 0.2s linear 0.5s,box-shadow 0.2s linear 0.5s,width 0s linear 0.7s;
    }
    
    & .navigation-fourth-tier {
      transition: visibility 0s linear 0.7s,opacity 0.2s linear 0.5s,box-shadow 0.2s linear 0.5s,width 0s linear 0.7s;
    }
  }
  
  .navigation-first-tier {
    position: relative;
   
    > li {
      display: inline-block;

      > a {
        /*padding: 15px 30px;*/
        padding: 1px 20px;
		color: white; /*added by yak for YPBF*/
        font-weight: 700; /*added by yak for YPBF*/
        font-size: 1em; /*added by yak for YPBF*/
        transition: all 0.2s linear 0.5s; /*added by yak for YPBF*/    
      }
      
      > a:hover {
        transition-delay: 0.2s; /*delay before dropdown menu becomes visible*/
    	transition-duration: 0s; /*make menus animation open immediately*/
      }
    }

    > .has-dropdown > a:after {
      content: '\E006';
      margin-left: 10px;
      @include icon-font();
    }

    > .has-dropdown:hover {
      z-index: 30;

      > a {
        /*border: 1px solid $border-color;*/
        border-bottom: none;
        /*padding: 14px 29px 15px;*/
        background: $body-background-color;
        /*z-index: 4;*/
        z-index: 8;
        border-radius: 5px 5px 0px 0px;
      }
    }
  }

  .navigation-second-tier,
  .navigation-third-tier {

    .has-dropdown > a:after {
      /*content: '\E008';*/
      /*content: '\003E';*//*Change plus signs in sub menus to greater than signs*/
      /*content: '\FE65';*//*Change plus signs in sub menus to smaller greater than signs*/
      content: "\203A";
      position: absolute;
      /*top: 50%;*/
      top: 55%;
      /*right: 15px;*/
      right: 10px;
      margin-top: -9px;
      /*@include icon-font(18px);*/
      @include icon-font(16px);
      font-weight: 700;
    }

    .has-dropdown:hover > a {
      border-right-color: $body-background-color;

      &:after {
        content: none;
      }

      .alternate-drop & {
        border-left-color: $body-background-color;
        border-right-color: $border-color;
      }
    }
  }

  .navigation-second-tier {
    top: 100%;
    margin-top: -1px !important; // override default margins

    > .has-dropdown:hover > a {
      z-index: 3;
    }
  }

  .navigation-third-tier {
    top: 0;
    /*left: 199px;*/
    left: 229px; /* Added by Yak */
    
    .alternate-drop & {
      left: auto;
      /*right: 199px;*/
      right: 229px; /* Added by Yak */
    }

    > .has-dropdown:hover > a {
      z-index: 2;
    }
  }

  .navigation-fourth-tier {
    top: 0;
    /*left: 199px;*/
    left: 229px; /* Added by Yak */

    .alternate-drop & {
      left: auto;
      /*right: 199px;*/
      right: 229px; /* Added by Yak */
    }

    > .has-dropdown:hover > a {
      z-index: 1;
    }
  }

  .mobile-tier-toggle {
    display: none;
  }

  /* Mega nav */

  li.mega-nav {
    position: static;

    li {
      position: static;
    }

    > a {
      position: relative;
      z-index: 55;

      &:after {
        content: '\E006';
        margin-left: 10px;
        @include icon-font();
      }
    }

    &:hover {

      > a {
        background: $body-background-color;
        border: 1px solid $border-color;
        border-bottom: none;
        padding: 14px 29px 15px;
      }

      .mega-nav-dropdown {
        display: block;
      }
    }
  }

  ul.mega-nav-dropdown {
    display: none;
    width: $max-width;
    position: absolute;
    padding: 0 15px;
    left: 50%;
    margin-left: -630px;
    background: $body-background-color;
    border: 1px solid $border-color;
    text-align: left;
    font-size: 0;
    z-index: 50;
    overflow: hidden;

    a:hover {
      color: $accent-color;
    }

    ul ul {
      display: none;
    }
  }

  li.mega-nav-item {
    display: inline-block;
    vertical-align: top;
    width: 100%;
    padding: 30px 15px;
    font-size: 14px;

    &:hover > a {
      color: $body-color;
    }

    > a:hover {
      color: $accent-color;
    }

    .has-2-items & {
      width: 50%;
    }

    .has-3-items & {
      width: 33%;
    }

    .has-4-items & {
      width: 25%;
    }

    .meta {
      font-size: 14px;
    }

    .mega-nav-item-image-wrapper + .meta,
    img + .meta {
      display: inline-block;
      margin-top: 15px;
    }

    > ul {
      display: block;
      border-top: 1px solid $border-color;
      margin-top: 15px;
    }

    ul a {
      padding-top: 15px;
    }
  }

  .mega-nav-item .has-children {

    > a:after {
      content: '\E008';
      margin-left: 5px;
      @include icon-font();
    }
  }

  .mega-nav-item-image {
    display: block;
  }

  ul.mega-nav-second-tier,
  ul.mega-nav-third-tier {
    display: none;
    position: absolute;
    width: 100%;
    padding: 30px;
    top: 0;
    left: 0;
    background: $body-background-color;
    font-size: 0;

    li {
      width: 25%;
      display: inline-block;
      vertical-align: top;
      font-size: 14px;
      padding-right: 20px;
    }

    .working + & {
      display: block;
      visibility: hidden;
    }

    .active + & {
      display: block;
      visibility: visible;
    }
  }

  ul.mega-nav-second-tier {
    z-index: 50;
  }

  ul.mega-nav-third-tier {
    z-index: 100;
  }

  ul li.mega-nav-breadcrumbs {
    @extend .meta;
    width: 100%;
    display: block;
    border-bottom: 1px solid $border-color;
    padding-bottom: 15px;
    padding-right: 0;

    span {
      color: $accent-color;
      cursor: pointer;

      &:hover {
        color: hover($body-background-color, $accent-color);
      }

      &:after {
        content: '/';
        margin: 0 2px 0 8px;
        color: $body-color;
      }
    }

    .current-tier {
      color: $body-color;
      cursor: default;

      &:after {
        content: none;
      }

      &:hover {
        color: $body-color;
      }
    }

    .previous-tier {
      float: right;
      color: $meta-color;
      cursor: pointer;
      font-size: 12px;

      &:hover {
        color: hover($body-background-color, $meta-color);
      }

      &:after {
        content: none;
      }
    }
  }

}

