/* General
-------------------------------- */

body {
  font-family: $body-font;
  color: $body-color;
  font-size: 16px;
  line-height: 1.625;
  -webkit-font-smoothing: antialiased;
}

/* Links */

a {
  color: $accent-color;
  text-decoration: none;

  &:hover {
    color: hover($body-background-color, $accent-color);
  }
}

/* Headers */

h1,
h2,
h3,
h4,
h5,
h6 {
  line-height: 1.25;
  font-family: $heading-font;
  font-weight: $heading-weight;
  color: $heading-color;

  {% if settings.heading-font-small-caps %}
    letter-spacing: 0.1em;
    text-transform: uppercase;
  {% endif %}
}

h1 {
  margin-top: 1.1em;
  margin-bottom: 0.4em;

  {% if settings.heading-font-small-caps %}
    font-size: 30px;
  {% else %}
    font-size: 36px;
  {% endif %}
}

h2 {
  margin-top: 1.22em;
  margin-bottom: 0.9em;

  {% if settings.heading-font-small-caps %}
    font-size: 20px;
  {% else %}
    font-size: 24px;
  {% endif %}
}

h3 {
  margin-top: 1.7em;
  margin-bottom: 0.75em;

  {% if settings.heading-font-small-caps %}
    font-size: 17px;
  {% else %}
    font-size: 20px;
  {% endif %}
}

h4,
h5,
h6 {
  margin: 1.7em 0 1em;

  {% if settings.heading-font-small-caps %}
    font-size: 14px;
  {% else %}
    font-size: 16px;
  {% endif %}
}

.page-title {
  margin: 35px 0;
  text-align: center;
}

.section-title {
  font-family: $section-heading-font;
  font-weight: $section-heading-weight;
  font-size: 17px;
  letter-spacing: 0.1em;
  text-transform: uppercase;
}

.meta {
  color: $meta-color;
  font-family: $meta-font;
  font-weight: $meta-weight;
  font-size: 14px;
  letter-spacing: 0.1em;
  text-transform: uppercase;
}

/* Basic elements */

p,
ul,
ol,
dl,
blockquote,
table,
code,
figure {
  margin-top: 1.3em;
  margin-bottom: 1.3em;
}

figure {
  margin-left: 0;
  margin-right: 0;
}

img {
  max-width: 100%;
}

ul,
ol,
dl {
  padding-left: 22px;
}

ul {
  list-style: outside none;
}

ol {
  list-style: outside decimal;
}

li {

}

hr {
  height: 2px;
  background: $border-color;
  border: none;
}

blockquote {
  margin: 0;
  color: $heading-color;

  > *:first-child {
    margin-top: 0;
  }

  > *:last-child {
    margin-bottom: 0;
  }

  &.highlight {
    font-size: 24px;
    line-height: 38px;
    margin-left: -50px;
    margin-right: -50px;

    @include breakpoint(m) {
      margin-left: 0;
      margin-right: 0;
    }
  }

}

