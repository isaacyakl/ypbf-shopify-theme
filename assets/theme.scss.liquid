.slideshow-slide {
  position: absolute;
  width: 100%;
  box-shadow: 0px 0px 5px 0px grey; /*added by Yak*/
}

.slide-link {
  @extend %cover;
  z-index: 1;
}

.slide-image {
  width: 100%;
  display: block;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
}

.transition-fade .slideshow-slide {
  opacity: 0;

  &.active {
    opacity: 1;
    z-index: 1;
  }

  &.next.left,
  &.prev.right,
  &.active.left,
  &.active.right {
    @include transition(opacity $slideshowSpeed $ease-out-quad);
  }

  &.next.left,
  &.prev.right {
    opacity: 1;
  }

  &.active.left {
    opacity: 0;
  }
  &.active.right {
    opacity: 0;
  }
}

.transition-slide .slideshow-slide {
  visibility: hidden;

  &.active {
    visibility: visible;
    z-index: 1;
    @include transform(translateX(0));
  }

  &.next {
    @include transform(translateX(100%));
  }

  &.prev {
    @include transform(translateX(-100%));
  }

  &.next.left,
  &.prev.right,
  &.active.left,
  &.active.right {
    @include transition(transform $slideshowSpeed $ease-out-quad);
  }

  &.next.left,
  &.prev.right {
    @include transform(translateX(0));
    visibility: visible;
    z-index: 10;
  }

  &.active.left {
    @include transform(translateX(-100%));
  }
  &.active.right {
    @include transform(translateX(100%));
  }
}

.slide-content {
  position: absolute;
  top: 50%;
  left: 120px;
  right: 120px;
  @include transform(translateY(-50%));

  .text-shadows-true & {
    text-shadow: 0px 0px 10px rgba(0,0,0,0.2);

