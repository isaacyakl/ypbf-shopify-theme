      background: white;

      @include transition(all 0.2s $ease-out-quad);
    }

    &:before {
      width: 14px;
      height: 2px;
      top: 17px;
      left: 11px;
    }

    &:after {
      width: 2px;
      height: 14px;
      top: 9px;
      left: 17px;
    }

    .open > a > &,
    .open > .tier-toggle-wrap > & {
      &:after, &:before {
        @include transform(rotate(90deg));
      }

      &:before {
        opacity: 0;
      }
    }
  }

  /* Reset mega nav styles */

  .mega-nav-item-image-wrapper,
  .mega-nav-item-image,
  .mega-nav-breadcrumbs {
    display: none !important;
  }

  .navigation .meta {
    font-family: $body-font;
    text-transform: none;
    letter-spacing: 0;
    color: inherit;
  }

}

@include breakpoint(m) {

  .navigation {
    left: -15px;
    right: -15px;
    margin-top: 25px;
    padding-top: 0px;
    width: 95%; /* Added by Yak */
  }
}

.pagination {
  @extend .meta;
  /*margin: 90px 0;*/ /* Commented out by Yak*/
  margin: 25px 0;
  padding: 0;
  text-align: center;

  li {
    display: inline-block;
    margin: 0 15px;
    color: $meta-color-secondary;
  }

  .pagination-position,
  a,
  a:before,
  a:after {
    color: $meta-color;
  }

  @include breakpoint(s) {
    margin: 45px 0;
    @include display(flex);
    @include flex-wrap(wrap);
    @include justify-content(center);
  }
}

ul li.pagination-position {
  @include breakpoint(s) {
    @include order(3);
    display: block;
    width: 100%;
    margin-top: 15px;
  }
}

.pagination-previous *:before {
  content: '\E013';
  padding-right: 5px;
  @include icon-font();
}

.pagination-next *:after {
  content: '\E014';
  padding-left: 5px;
  @include icon-font();
}

.template-customers-addresses .pagination {
  margin: 0 0 35px;
}

.share-buttons-container { /*Added by Yak*/
    position: relative;
    text-align: center;
  	width: initial;
  	margin-bottom: 0px;
    
    @include breakpoint(m) {
    	width: 100%;
      	margin-bottom: 15px;
  	}
}

.share-buttons {
  font-size: 0;
  margin: 45px -5px 0;

  a {
    display: inline-block;
    padding: 6px 13px;
    border-radius: 5px;
    border: 1px solid $border-color;
    color: $body-color;
    margin: 5px;
    letter-spacing: 0;

    @include breakpoint(s) {
      padding: 6px 11px;
    }

    .template-blog &,
    .template-article & {
      margin-right: 0;
    }

    &:before {
      font-family: 'Social Icons';
      -webkit-font-smoothing: antialiased;
      font-size: 18px;
      font-weight: normal;
      line-height: 1;

      @include transition(color 0.3s $ease-out-quad);
    }

    &:hover {
      color: white;

      &:before {
        color: white;
      }
    }
  }
}

.share-facebook {

  &:hover {
    background: #49659D;
    border-color: #49659D;
  }

  &:before {
    content: '\E003';
    color: #49659D;
  }
}

.share-twitter {

  &:hover {
    background: #09AEEC;
    border-color: #09AEEC;
  }

  &:before {
    content: '\E036';
    color: #09AEEC;
  }
}

.share-pinterest {

  &:hover {
    background: #CB1F2A;
    border-color: #CB1F2A;
  }

  &:before {
    content: '\E296';
    color: #CB1F2A;
  }
}

.share-fancy {

  &:hover {
    background: #49659D;
    border-color: #49659D;
  }

  &:before {
    content: '\E430';
    color: #49659D;
  }
}

.share-google {

  &:hover {
    background: #CC3A2B;
    border-color: #CC3A2B;
  }

  &:before {
    content: '\E03F';
    color: #CC3A2B;
  }
}

.share-email {

  &:hover {
    background: #525252;
    border-color: #525252;
  }

  &:before {
    content: '\E050';
    color: #525252;
  }
}

.sidebar {
  width: 20%;
  min-width: 200px;
  padding-right: 30px;

  @include breakpoint(m) {
    width: 100%;
    margin-top: 45px;
    padding: 35px 0 0;
    border-top: 1px solid $border-color;
    font-size: 0;
  }
}

body[class*="template-index"].sidebar-enabled,
body[class*="template-list-collections"].sidebar-enabled,
body[class*="template-collection"].sidebar-enabled {

  .main-content {
    width: 80%;
    float: right;

    @media screen and (max-width: 1060px) {
      width: -webkit-calc(100% - 200px);
      width: calc(100% - 200px);
    }

    @include breakpoint(m) {
      width: 100%;
      float: none;
    }
  }
}

body[class*="template-list-collections"].sidebar-enabled {

  .sidebar {
    margin-top: 30px;
  }
}

.sidebar-link-list-title {
  margin-top: 35px;
  line-height: 1.625;

  &:first-child {
    margin-top: 0;
  }
}

.sidebar-link-list {
  margin-top: 35px;

  &:first-child {
    margin-top: 0;
  }

  @include breakpoint(m) {
    margin-top: 0;
  }

  ul {
    padding: 0;
    font-size: 14px;
    font-weight: bold;

    ul {
      margin: 0;
      padding-left: 10px;
      font-weight: normal;
    }
  }

  li {
    margin: 16px 0;
  }

  a {
    color: $heading-color;

    &:hover {
      color: hover($body-background-color, $heading-color);
    }
  }

  .active > a {
    color: $accent-color;

    &:after {
      content: '\E006';
      position: relative;
      top: 1px;
      margin-left: 7px;
      display: inline-block;

      @include transform(rotate(270deg));
      @include icon-font();

      @include breakpoint(m) {
        content: none;
      }

      @include lt-ie9 {
        display: none;
      }
    }
  }
}

.sidebar-link-list-mobile {
  display: none;

  @include breakpoint(m) {
    display: block;
  }
}

.sidebar-link-list {
  @include breakpoint(m) {
    width: 33%;
    display: inline-block;
    vertical-align: top;
    text-align: center;
    padding: 0 15px;

    ul {
      margin-bottom: 0;
      font-weight: normal;

      ul {
        padding-left: 0;
        font-weight: bold;
        display: none;
      }
    }

    .open > ul {
      display: block;
    }

    .has-children > a {
      position: relative;
    }

    .has-children > a:after {
      content: '\E008' !important; // override active link content: none;
      padding-left: 5px;
      @include transform(rotate(0));
      @include icon-font();
      -webkit-font-smoothing: initial;
    }

    .open.has-children > a:after {
      content: '\E009' !important; // override active link content: none;
    }

    li:last-child {
      margin-bottom: 0;
    }

    .active > a {
      color: $heading-color;

      &:after {
        content: none;
      }
    }
  }
}

.sidebar-link-list {
  @include breakpoint(s) {
    display: block;
    width: 100%;
    margin: 30px 0 0;
    padding: 0;

    &:first-child {
      margin-top: 0;
    }
  }
}


/* Extends */

/**
 * For modern browsers
 * 1. The space content is one way to avoid an Opera bug when the
 *    contenteditable attribute is included anywhere else in the document.
 *    Otherwise it causes space to appear at the top and bottom of elements
 *    that are clearfixed.
 * 2. The use of `table` rather than `block` is only necessary if using
 *    `:before` to contain the top-margins of child elements.
 */
.clearfix:before,
.clearfix:after {
    content: " "; /* 1 */
    display: table; /* 2 */
}

.clearfix:after {
    clear: both;
}

/**
 * For IE 6/7 only
 * Include this rule to trigger hasLayout and contain floats.
 */
.clearfix {
    *zoom: 1;
}

/* Small caps */

.small-caps {
  text-transform: uppercase;
  letter-spacing: 0.1em;
}

%cover {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
}

