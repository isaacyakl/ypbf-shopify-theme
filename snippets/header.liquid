<div class="main-header-wrapper">
  <header class="main-header clearfix" role="banner">

    <div class="tools">
            <!--<style>
            	#customer_login_link,#customer_login_link:hover,#customer_login_link:active,#customer_login_link:visited {
                	color: #a741d7;
                }
            </style>//-->
            
            
				{% if shop.customer_accounts_enabled %}
                  {% if customer %}
                    <a href="/account">{{ 'customers.account.title' | t }}</a>
                  {% else %}
                    {{ 'customers.account.title' | t | customer_login_link }}
                  {% endif %}
				{% endif %}
              
              <div class="header-tools">

                <a class="cart-count {% if cart.item_count < 100 %}contain-count{% endif %}" href="/cart">
                  <span class="bag-count">{{ cart.item_count }}</span>
                  <span class="bag-text">{{ 'layout.header.cart_item_count' | t: count: cart.item_count }}</span>
                </a>

                {% if settings.enable-currency-switcher %}
                  <div class="select-wrapper currency-switcher">
                    <div class="selected-currency"></div>
                    <select id="currencies" name="currencies">
                      {% capture codes %},USD,EUR,GBP,CAD,ARS,AUD,BBD,BDT,BSD,BHD,BRL,BOB,BND,BGN,ILS,MMK,KYD,CLP,CNY,COP,CRC,HRK,CZK,DKK,DOP,XCD,EGP,XPF,FJD,GHS,GTQ,GYD,GEL,HKD,HUF,ISK,INR,IDR,NIS,JMD,JPY,JOD,KZT,KES,KWD,LVL,LTL,MXN,MYR,MUR,MDL,MAD,MNT,MZN,ANG,NZD,NGN,NOK,OMR,PKR,PYG,PEN,PHP,PLN,QAR,RON,RUB,SAR,RSD,SCR,SGD,SYP,ZAR,KRW,LKR,SEK,CHF,TWD,THB,TZS,TTD,TRY,UAH,AED,UYU,VEB,VND,ZMK,{% endcapture %}
                      {% assign supported_codes = settings.currency-switcher-supported-currencies | split: ' ' %}
                      <option value="{{ shop.currency }}" selected="selected">{{ shop.currency }}</option>
                      {% for code in supported_codes %}
                        {% if code != shop.currency and codes contains code %}
                          <option value="{{ code }}">{{ code }}</option>
                        {% endif %}
                      {% endfor %}
                    </select>
                  </div>
                {% endif %}

              </div>
    </div>
    
    <div class="branding wow bounceInDown">
      {% if settings.use-logo %}
      	{% if template == 'index' %}<h1 style="margin: 0px; padding: 0px;">{% endif %}
        <a class="logo {% if settings.use-retina-logo %}has-retina{% endif %}" href="/">
          <img class="logo-regular" alt="{{ shop.name | escape }}" src="{{ 'logo.png' | asset_url }}">
          {% if settings.use-retina-logo %}
            <img class="logo-retina" alt="{{ shop.name | escape }}" src="{{ 'logo-retina.png' | asset_url }}">
          {% endif %}
        </a>
      	{% if template == 'index' %}</h1>{% endif %}
      {% else %}
        {% if template contains 'index' %}
          <h1 class="site-title">{{ shop.name }}</h1>
        {% else %}
          <div class="site-title"><a href="/">{{ shop.name }}</a></div>
        {% endif %}
      {% endif %}
    </div>
    
    <form class="header-search-form wow bounceInDown" action="/search" method="get">
      <input class="header-search-input" name="q" type="text" autofocus/>
    </form>
    
